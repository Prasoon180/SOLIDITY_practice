pragma solidity >= 0.5.0 < 0.9.0;
contract practice_3 {

    uint num = 10;
    function set(uint _item) public {                            // using set function
        num = _item;
    }

        function get() public view returns(uint) {               // using get function
            return num;
        }
    }
