pragma solidity >= 0.5.0 < 0.9.0;
contract practice_9 {
    function prime(uint256 n ) public pure returns(uint256)
    {
        if (n <= 1)
        {
            return 0;     // not prime
        }

        for(uint256 i = 2; i < n; i++)
        {
            if (n % i == 0) {
                return 0;                // not prime
                 }
        }
        return 1;   
                       }                   // prime
    }
